{
  "name": "kibana-pr-bot",
  "private": true,
  "license": "Apache 2.0",
  "main": "dist/run_server.js",
  "scripts": {
    "build": "babel src -d dist --delete-dir-on-start -x .ts --source-maps",
    "type_check": "tsc --noEmit",
    "lint": "yarn eslint . --ext .ts,.js",
    "test": "yarn type_check && yarn lint"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/*.test.[jt]s?(x)"
    ],
    "setupFiles": [
      "dotenv/config"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@elastic/elasticsearch": "^7.1.0",
    "@spalger/micro-plus": "^0.0.17",
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "elastic-apm-node": "^2.10.0",
    "execa": "^2.0.4",
    "github-webhook-event-types": "^1.2.1",
    "graphql": "^14.5.6",
    "graphql-tag": "^2.10.1",
    "lodash.throttle": "^4.1.1",
    "parse-link-header": "^1.0.1",
    "source-map-support": "^0.5.12",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@types/execa": "^2.0.0",
    "@types/jest": "^24.0.18",
    "@types/lodash.throttle": "^4.1.6",
    "@types/micro": "^7.3.3",
    "@types/node": "12.7.2",
    "@types/parse-link-header": "^1.0.0",
    "@types/user-home": "^2.0.0",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.9.0",
    "babel-jest": "^24.9.0",
    "prettier": "^1.16.4",
    "typescript": "^3.4.1"
  },
  "engines": {
    "node": "11"
  }
}
